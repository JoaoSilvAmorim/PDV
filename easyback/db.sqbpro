t_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT  id,
		CASE 
		WHEN strftime('%m', data) = '01' THEN 'Janeiro' 
		WHEN strftime('%m', data) = '02' THEN 'Fevereiro' 
		WHEN strftime('%m', data) = '03' THEN 'Mar√ßo'
		WHEN strftime('%m', data) = '04' THEN 'Abril'
		WHEN strftime('%m', data) = '05' THEN 'Maio' 
		WHEN strftime('%m', data) = '06' THEN 'Junho'
		WHEN strftime('%m', data) = '07' THEN 'Julho' 
		WHEN strftime('%m', data) = '08' THEN 'Agosto' 
		WHEN strftime('%m', data) = '09' THEN 'Setembro'
		WHEN strftime('%m', data) = '10' THEN 'Outubro' 
		WHEN strftime('%m', data) = '11' THEN 'Novembro'
		WHEN strftime('%m', data) = '12' THEN 'Dezembro'
		ELSE NULL END mes,
        sum(total) AS soma, data, count('*') AS quantidade 
		FROM(
        SELECT id, totalAjusteGeral AS total, dataConfirmacao AS data FROM royal_ajusteestoque WHERE situacao = 'Aberta' AND tipo = '1'
        UNION ALL 
        SELECT id, valorVenda AS total, dataPagamento AS data FROM royal_vendapdv WHERE formaPagamento = 'Credito'
        )WHERE data BETWEEN date('now', '-1 year') AND date('now', '+1 year') GROUP BY  strftime('%Y-%m', data)</sql><current_tab id="0"/></tab_sql></sqlb_project>
